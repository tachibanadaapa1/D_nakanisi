{
  "name": "front-template",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.13",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.9.0",
    "gulp-inject": "^4.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-template": "^4.0.0",
    "gulp-watch": "^4.3.11",
    "js-beautify": "^1.7.4",
    "prettier-eslint": "^8.2.2",
    "prettier-eslint-cli": "^4.4.0",
    "rimraf": "^2.6.2",
    "run-sequence": "^2.2.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-scss": "^2.1.0",
    "webpack": "^3.8.1",
    "webpack-stream": "^4.0.0"
  },
  "scripts": {
    "build": "gulp",
    "dev-server": "gulp dev-server",
    "lint": "yarn lint:css && yarn lint:scss: && yarn lint:js",
    "lint:css": "stylelint \"app/css/**/*.css\"",
    "lint:scss": "stylelint \"app/scss/**/*.scss\"",
    "lint:js": "eslint \"app/js/**/*.js\" \"./*.js\"",
    "fmt": "yarn fmt:scss && yarn fmt:html && yarn fmt:css && yarn fmt:js",
    "fmt:html": "js-beautify --type html -r app/*.html",
    "fmt:css": "stylefmt --recursive \"app/css/**/*.css\"",
    "fmt:scss": "stylefmt --recursive \"app/scss/**/*.scss\"",
    "fmt:js": "prettier-eslint --write \"app/js/**/*.js\" \"./*.js\""
  }
}
